{
  "name": "ctrl-alt-rock-voting",
  "version": "1.0.0",
  "description": "Online Voting System for CTRL + ALT + ROCK Event",
  "main": "server/server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "node server/server.js",
    "install-all": "npm install && cd client && npm install"
  },
  "keywords": ["voting", "competition", "web-app", "mobile-friendly"],
  "author": "Event Organizers",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "qrcode": "^1.5.3",
    "csv-parser": "^3.0.0",
    "express-session": "^1.17.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  }
}